{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "Status",
    "type": "object",
    "properties": {
        "key": {"type": "string"},
        "status": {
            "enum": ["queued", "running", "success", "error", "orphan", "interrupted", "redirect", "missing"]
        },
        "version": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "$ref": "OrderlyId.schema.json"
                }
            ]
        },
        "output": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "type": "object",
                    "properties": {
                        "stdout": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "stderr": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "required": ["stdout", "stderr"],
                    "additional_properites": false
                }
            ]
        },
        "task_position": { "type": "integer" }
    },
    "required": ["key", "status", "version", "output"],
    "additionalProperties": false
}
