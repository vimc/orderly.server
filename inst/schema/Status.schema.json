{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "Status",
    "type": "object",
    "properties": {
        "key": {"type": "string"},
        "status": {
            "enum": ["queued", "running", "success", "error", "orphan", "interrupted", "redirect", "missing"]
        },
        "version": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "$ref": "OrderlyId.schema.json"
                }
            ]
        },
        "output": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            ]
        },
        "queue": { "type": "integer" }
    },
    "required": ["key", "status", "version", "output"],
    "additionalProperties": false
}
